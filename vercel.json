{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "web/build" }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/static/(.*)", "dest": "/web/build/static/$1" },
    { "src": "/asset-manifest\\.json", "dest": "/web/build/asset-manifest.json" },
    { "src": "/favicon\\.ico", "dest": "/web/build/favicon.ico" },
    { "src": "/manifest\\.json", "dest": "/web/build/manifest.json" },
    { "src": "/logo192\\.png", "dest": "/web/build/logo192.png" },
    { "src": "/logo512\\.png", "dest": "/web/build/logo512.png" },
    { "src": "/robots\\.txt", "dest": "/web/build/robots.txt" },
    { "src": "/(.*)", "dest": "/web/build/index.html" }
  ]
}
