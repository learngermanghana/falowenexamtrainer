{
  "version": 2,
  "installCommand": "npm install && npm --prefix web install",
  "buildCommand": "npm --prefix web run build",
  "outputDirectory": "web/build",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/sync-student", "dest": "/api/sync-student.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
